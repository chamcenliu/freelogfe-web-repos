{ 
  "compilerOptions": {
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "./built",
    "removeComments": true,
    "sourceMap": true,
    "target": "ES5"
  },
  "exclude": [
    "dist", "typings*",
    "src/i18n*",
    "src/app*",
    "node_modules*",
    "test"
  ]
}